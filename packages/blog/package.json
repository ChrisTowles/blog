{
  "name": "@chris-towles/blog",
  "private": true,
  "author": "Chris Towles<Chris.Towles@gmail.com>",
  "maintainers": [
    "Chris Towles<Chris.Towles@gmail.com>"
  ],
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "build:cf": "nuxt build --preset=cloudflare_pages",
    "dev:no-remote": "nuxt dev",
    "dev": "nuxt dev --remote --envName development",
    "dev:debug": "DEBUG=*:*:*,vite:*:*  nuxt dev --remote",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "typecheck": "nuxt typecheck",
    "test": "vitest",
    "ai-example": "tsx scripts/cloudflare-ai-example.ts",
    "test-ai": "tsx scripts/test-cloudflare-ai.ts",
    "prepare": "today=$(date +%Y-%m-%d) && echo \"compatibility_date = '$today'\" > ./.data/hub/wrangler.toml && wrangler --config ./.data/hub/wrangler.toml types",
    "deploy:main": "wrangler pages deploy",
    "deploy:nuxthub": "npx nuxthub deploy",
    "deploy:preview": "wrangler pages dev",
    "cleanup": "rm -rf .data/content/contents.sqlite && npx nuxi cleanup",
    "db:migrate": "drizzle-kit migrate"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^1.2.11",
    "@ai-sdk/vue": "^1.2.12",
    "@aws-sdk/credential-providers": "^3.806.0",
    "@cloudflare/ai-utils": "^1.0.1",
    "@nuxt/ui-pro": "^3.1.1",
    "@nuxthub/core": "^0.9.0",
    "@nuxtjs/mdc": "^0.17.0",
    "@tanstack/vue-virtual": "^3.13.8",
    "ai": "^4.3.15",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.43.1",
    "nuxt": "^3.17.2",
    "nuxt-auth-utils": "^0.5.20",
    "nuxt-og-image": "^5.1.3",
    "shiki-stream": "^0.1.2",
    "ufo": "^1.6.1",
    "workers-ai-provider": "^0.3.2",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@iconify-json/heroicons": "^1.2.2",
    "@iconify-json/logos": "^1.2.4",
    "@iconify-json/lucide": "^1.2.41",
    "@iconify-json/simple-icons": "^1.2.33",
    "@nuxt/content": "^3.5.1",
    "@nuxt/eslint": "^1.3.0",
    "@nuxt/fonts": "^0.11.3",
    "@nuxt/icon": "^1.12.0",
    "@nuxt/image": "^1.10.0",
    "@nuxt/test-utils": "^3.18.0",
    "@nuxtjs/sitemap": "^7.2.10",
    "@types/node": "^22.15.17",
    "@vue/test-utils": "^2.4.6",
    "@vueuse/core": "^13.1.0",
    "@vueuse/nuxt": "^13.1.0",
    "dotenv": "^16.5.0",
    "drizzle-kit": "^0.31.1",
    "eslint": "^9.26.0",
    "happy-dom": "^17.4.6",
    "nuxt-gtag": "^3.0.2",
    "playwright-core": "^1.52.0",
    "tsx": "^4.19.2",
    "typescript": "^5.8.3",
    "vitest": "^3.1.3",
    "vue-tsc": "^2.2.10",
    "wrangler": "^4.14.4"
  }
}
